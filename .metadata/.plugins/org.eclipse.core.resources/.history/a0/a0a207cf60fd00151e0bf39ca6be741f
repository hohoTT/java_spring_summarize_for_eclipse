package com.wt.spring.beans;

public class Main {

	public static void main(String[] args) {
		
		// 一. 以下为没有使用 spring 执行的 hello 的显示效果
		// 创建 HelloWorld 的一个对象
		HelloWorld helloWorld = new HelloWorld();
		
		// 为 name 属性赋值
		helloWorld.setName("hohoTT");
		
		// 调用 hello 方法
		helloWorld.hello();
		
		// 二、 以下为使用 spring 执行的 hello 的显示效果
		
	}
	
}
